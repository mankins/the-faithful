{
  "name": "functions",
  "description": "Cloud Functions for Firebase",
  "scripts": {
    "serve": "firebase emulators:start --project the-faithful",
    "shell": "firebase functions:shell",
    "start": "npm run shell",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log",
    "pretty": "prettier --write .",
    "lint": "eslint --ext .js .",
    "lint:fix": "eslint --fix --ext .js .",
    "pubsub": "gcloud beta emulators pubsub start"
  },
  "engines": {
    "node": "14"
  },
  "main": "index.js",
  "dependencies": {
    "@google-cloud/logging": "^9.0.1",
    "@google-cloud/secret-manager": "^3.4.0",
    "@google-cloud/video-transcoder": "^1.2.0",
    "base58": "^2.0.1",
    "btoa": "^1.2.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "firebase-admin": "^9.2.0",
    "firebase-functions": "^3.13.1",
    "lodash.get": "^4.4.2",
    "node-fetch": "^2.6.1",
    "qs": "^6.9.6",
    "stripe": "^8.136.0",
    "uuid": "^8.3.2",
    "uuid-encoder": "^1.2.0"
  },
  "devDependencies": {
    "eslint": "^7.18.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "firebase-functions-test": "^0.2.0",
    "prettier": "^2.2.1"
  },
  "private": true
}
